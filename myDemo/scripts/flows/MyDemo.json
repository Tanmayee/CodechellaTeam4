
{
   "model" : {
        "name" : "FlowTestModel"
    },
    
   "startState":"start",
   "start":{
       "state_type":"VIEW",
       "ref":"home",
       "transitions":{
           "find":"getName",
           "aboutLaw" : "aboutlaw"
       }
   },
   
   
      
     "aboutlaw":{
       "state_type":"VIEW",
       "ref":"aboutTheLaw",
       "transitions":{
           "back": "start"
           
       }
   },
   
     "getName":{
       "state_type":"VIEW",
       "ref":"name",
       "transitions":{
           "next":"stateFlow"
       }
   },
   
   "stateFlow":{
   		"state_type":"ACTION",
   		"exp" : "$[myModel.fromCali] eq 'yes'",
        "transitions" : {
            "true" : "overView",
            "false" : "failPage"
        }
   },
   
    "failPage" : {
        "state_type" : "VIEW",
        "ref" : "Fail",
        "transitions" : {
            "back" : "start"
        }
    },
   
   "overView":{
       "state_type":"VIEW",
       "ref":"flowOverview",
       "transitions":{
           "back":"getName",
           "next":"married"
       }
   },
   
    "married":{
       "state_type":"VIEW",
       "ref":"maritalStatus",
       "transitions":{
           "back":"overView",
           "next":"getAge"
       }
   },
   
   "getAge":{
       "state_type":"VIEW",
       "ref":"age",
       "transitions":{
           "back":"married",
           "next":"getDependents"
       }
   },
   
    "getDependents":{
       "state_type":"VIEW",
       "ref":"dependents",
       "transitions":{
           "back":"getAge",
           "next":"employment"
       }
   },
   
    "employment":{
       "state_type":"VIEW",
       "ref":"employed",
       "transitions":{
           "back":"getDependents",
           "next":"employeeCoverage"
       }
   },
   
    "employeeCoverage":{
       "state_type":"ACTION",
       "exp" : "$[myModel.fromCali] eq 'yes'",
        "transitions" : {
            "true" : "premium",
            "false" : "failPage"
        }
   },
   
   "premium":{
       "state_type":"VIEW",
       "ref":"expectedPremium",
       "transitions":{
           "back":"employment",
           "next":"getIncome"
       }
   },
   
    "getIncome":{
       "state_type":"VIEW",
       "ref":"income",
       "transitions":{
           "back":"premium",
           "next":"showResults"
       }
   },
  
  
   "showResults":{
       "state_type":"VIEW",
       "ref":"results",
       "transitions":{
           "back":"getIncome",
           "next":"end_done"
       }
   },
  
   "end_done":{
       "state_type":"endDemo",
       "outcome":"doneWithDemo"
   }
}